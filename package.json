{
  "name": "celerik-nestjs-boilerplate",
  "version": "0.0.1",
  "description": "",
  "author": "Celerik",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
    "entity:create": "ts-node node_modules/.bin/typeorm entity:create --config ormconfig.js --name",
    "subscriber:create": "ts-node node_modules/.bin/typeorm subscriber:create --config ormconfig.js --name",
    "migration:create": "ts-node node_modules/.bin/typeorm migration:create --config ormconfig.js --name",
    "migration:generate": "ts-node node_modules/.bin/typeorm migration:generate --config ormconfig.js --name",
    "migration:run": "ts-node node_modules/.bin/typeorm migration:run --config ormconfig.js",
    "migration:revert": "ts-node node_modules/.bin/typeorm migration:revert --config ormconfig.js",
    "seed:config": "ts-node ./node_modules/typeorm-seeding/dist/cli.js config",
    "schema:drop": "ts-node node_modules/.bin/typeorm schema:drop --config ormconfig.js",
    "seed:run": "ts-node ./node_modules/typeorm-seeding/dist/cli.js seed",
    "seed:clear-run": "npm run schema:drop && npm run migration:run && npm run seed:run",
    "db:run": "npm run migration:revert && npm run migration:run && npm run seed:run",
    "check-types": "tsc",
    "prepare": "husky install",
    "pre-commit-lint": "npx husky add .husky/pre-commit 'npx --no-install lint-staged && npm run lint && npm run format && npm run build && git add .'",
    "commit-msg-lint": "npx husky add .husky/commit-msg 'npx --no-install commitlint --edit \"$1\"'"
  },
  "dependencies": {
    "@nestjs-modules/mailer": "1.6.0",
    "class-transformer": "0.5.1",
    "class-validator": "0.13.2",
    "@nestjs/axios": "0.0.1",
    "@nestjs/cli": "8.1.1",
    "@nestjs/common": "8.0.6",
    "@nestjs/config": "1.0.1",
    "@nestjs/core": "8.0.6",
    "@nestjs/jwt": "8.0.0",
    "@nestjs/mapped-types": "^1.0.0",
    "@nestjs/passport": "8.0.1",
    "@nestjs/platform-express": "8.0.6",
    "@nestjs/schedule": "1.0.1",
    "@nestjs/swagger": "^5.0.9",
    "@nestjs/typeorm": "8.0.2",
    "bcrypt": "5.0.1",
    "bcryptjs": "2.4.3",
    "dotenv": "10.0.0",
    "faker": "5.5.3",
    "handlebars": "4.7.7",
    "jsonwebtoken": "8.5.1",
    "moment": "2.29.1",
    "multer": "1.4.3",
    "nodemailer": "6.6.3",
    "passport": "0.4.1",
    "passport-jwt": "4.0.0",
    "pg": "8.7.1",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "7.3.0",
    "swagger-ui-express": "4.1.6",
    "typeorm": "0.2.36",
    "typeorm-seeding": "1.6.1"
  },
  "devDependencies": {
    "@commitlint/cli": "13.1.0",
    "@commitlint/config-conventional": "13.1.0",
    "@nestjs/schematics": "8.0.2",
    "@nestjs/testing": "8.0.6",
    "@types/bcrypt": "5.0.0",
    "@types/bcryptjs": "2.4.2",
    "@types/cron": "1.7.3",
    "@types/dotenv": "8.2.0",
    "@types/express": "4.17.13",
    "@types/faker": "5.5.7",
    "@types/jest": "27.0.0",
    "@types/multer": "1.4.7",
    "@types/node": "16.9.1",
    "@types/passport": "1.0.6",
    "@types/passport-jwt": "3.0.5",
    "@types/supertest": "2.0.10",
    "@typescript-eslint/eslint-plugin": "4.29.1",
    "@typescript-eslint/parser": "4.29.1",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "3.3.1",
    "husky": "7.0.0",
    "jest": "27.0.4",
    "lint-staged": "11.1.1",
    "prettier": "2.3.2",
    "sonarqube-scanner": "2.8.1",
    "supertest": "6.1.3",
    "ts-jest": "27.0.4",
    "ts-loader": "9.2.5",
    "ts-mapper": "1.2.4",
    "ts-morph": "11.0.3",
    "ts-node": "10.2.1",
    "tsconfig-paths": "3.9.0",
    "typescript": "4.4.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      ".+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
